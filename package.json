{
  "name": "social-proof-section-master",
  "version": "1.0.0",
  "description": "Social proof section challenge by Frontend Mentor",
  "main": "index.js",
  "homepage": "https://starskynder.github.io/social-proof-section/",
  "scripts": {
    "dev": "rm -rf ./development -rf ./.cache && parcel public/index.html --out-dir development -p 3000",
    "build": "parcel build public/index.html --out-dir dist --public-url /social-proof-section",
    "deploy": "gh-pages -d dist"
  },
  "author": "starskynder",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/plugin-transform-runtime": "^7.12.10",
    "@babel/runtime-corejs2": "^7.12.5",
    "autoprefixer": "^9.8.6",
    "gh-pages": "^3.1.0",
    "node-sass": "^5.0.0",
    "parcel-bundler": "^1.12.4",
    "postcss": "^8.2.4"
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {
        "overrideBrowserslist": [
          "> 1%",
          "last 4 versions",
          "ie >= 9"
        ]
      }
    }
  },
  "dependencies": {
    "node-forge": "^0.10.0"
  }
